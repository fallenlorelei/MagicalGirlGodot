[application]

config/name="Magical Girl Godot"
run/main_scene="res://tilemap.tscn"
config/icon="res://icon.png"

[autoload]

ElementalCrystalCounter="*res://ui/ElementalCrystalCounter.tscn"
DataImport="*res://data/DataImport.gd"
CooldownTracker="*res://abilities/CooldownTracker.tscn"
PlayerStats="*res://entities/PlayerStats.tscn"

[display]

window/size/width=854
window/size/height=480
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor]

version_control_autoload_on_startup=true
version_control_plugin_name="GitAPI"

[editor_plugins]

enabled=PoolStringArray( "res://addons/RPG_maker_Autotile2_3x3/plugin.cfg", "res://addons/color-palette/plugin.cfg" )

[gdnative]

singletons=[ "res://addons/godot-git-plugin/git_api.gdnlib" ]

[gui]

common/drop_mouse_on_gui_input_disabled=true

